{
  "version": 2,
  "builds": [
    {
      "src": "dist/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/src/index.js"
    },
    {
      "src": "/reference",
      "dest": "/dist/src/index.js"
    },
    {
      "src": "/swagger",
      "dest": "/dist/src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/src/index.js"
    }
  ]
}
